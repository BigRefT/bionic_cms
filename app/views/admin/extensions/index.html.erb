<h1>Extensions</h1>
<%= render_region :top %>
<div class="list_content">
  <%= render_region :before_list %>
  <table class="extension_index list_table" id="extension_index_id">
    <thead>
      <tr>
        <% render_region :list_headers do |list_headers| %>
          <% list_headers.name_column_header do %><th>Extension</th><% end %>
          <% list_headers.website_column_header do %><th>Website</th><% end %>
          <% list_headers.version_column_header do %><th>Version</th><% end %>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <%= render :partial => 'extension', :collection => @extensions %>
    </tbody>
  </table>
  <% render_region :after_list do |after_list| %>
    <% after_list.pagination_links do %>
      <%= empty_pagination_div %>
    <% end %>
  <% end %>
  <div class="action_row">
    <%= render_region :action_row %>
  </div>
</div>
<%= render_region :bottom %>